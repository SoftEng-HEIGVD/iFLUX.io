      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "configurationSchema": {
      "type": "string"
    },
    "callbackUrl": {
      "type": "string"
    },
    "callbackToken": {
      "type": "string"
    },
    "public": {
      "type": "boolean"
    },
    "organizationId": {
      "type": "integer"
