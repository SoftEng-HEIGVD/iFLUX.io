extends layout

append menu
	a(href='/') Overview
	a(href='/reference') Resources
	if resources
		each resource in resources
			a.level-1(href='/reference/##{resource.uniqueId}')
				span=resource.displayName ? resource.displayName : resource.relativeUri
				if resource.isPrivate
					i.glyphicon.glyphicon-lock
	else
		label Could not find resources list.